{
    "name": "fox/laravel-user-management",
    "description": "Fox's user management for Laravel applications.",
    "keywords": [
        "laravel",
        "application",
        "user",
        "permissions",
        "management",
        "user-management",
        "spatie"
    ],
    "license": "MIT",
    "type": "library",
    "version": "0.1.1",
    "require": {
        "php": ">=7.4.0",
        "illuminate/contracts": "^6.0.0|^7.0.0|^8.0.0",
        "illuminate/database": "^6.0.0|^7.0.0|^8.0.0",
        "illuminate/support": "^6.0.0|^7.0.0|^8.0.0",
        "illuminate/auth": "^6.0.0|^7.0.0|^8.0.0",
        "illuminate/console": "^6.0.0|^7.0.0|^8.0.0",
        "illuminate/config": "^6.0.0|^7.0.0|^8.0.0",
        "spatie/laravel-permission": "^4.0",
        "dmx/support-toolset": "^3.3"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "friendsofphp/php-cs-fixer": "^2.18",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Fox\\UserManagement\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fox\\UserManagement\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "style-fix": "vendor/bin/php-cs-fixer fix",
        "style-check": "vendor/bin/php-cs-fixer fix --dry-run --using-cache=no"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fox\\UserManagement\\ServiceProvider"
            ]
        }
    }
}
